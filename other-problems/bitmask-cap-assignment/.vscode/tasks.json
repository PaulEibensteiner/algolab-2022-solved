  "version": "2.0.0",
  "tasks": [
    {
      "label": "run",
      "type": "shell",
      "command": "~/.algolabVS.sh",
	  "args": [
        "run"
      ],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "label": "test",
      "type": "shell",
      "command": "~/.algolabVS.sh",
	  "args": [
        "test"
      ],
      "group": "test"
    },
    {
      "label": "debug",
      "type": "shell",
      "command": "~/.algolabVS.sh",
	  "args": [
        "debug"
      ],
      "group": "none"
    },
  ]
}
